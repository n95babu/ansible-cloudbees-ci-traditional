[all:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[operations_center]
oc ansible_ssh_host=192.168.32.11 ansible_ssh_port=22 ansible_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/oc/virtualbox/private_key

[client_controllers]
cc1 ansible_ssh_host=192.168.32.13 ansible_ssh_port=22 ansible_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/cc1/virtualbox/private_key
cc2 ansible_ssh_host=192.168.32.15 ansible_ssh_port=22 ansible_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/cc2/virtualbox/private_key