[all:vars]
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[operations_center]
oc ansible_ssh_host=192.168.32.11 ansible_ssh_port=22 ansible_ssh_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/oc/virtualbox/private_key

[client_masters]
cm1 ansible_ssh_host=192.168.32.13 ansible_ssh_port=22 ansible_ssh_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/cm1/virtualbox/private_key

[agents]
agent1 ansible_ssh_host=192.168.32.15 ansible_ssh_port=22 ansible_ssh_user=vagrant ansible_ssh_private_key_file=.vagrant/machines/agent1/virtualbox/private_key